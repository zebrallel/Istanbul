{
    "presets": ["react", "stage-0", "es2015"],
    "plugins": [
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": true,
                "regenerator": true
            }
        ],
        ["transform-class-properties"],
        [
            "import",
            {
                "libraryName": "antd",
                "style": "css"
            }
        ],
        ["transform-decorators-legacy"]
    ]
}
